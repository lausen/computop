<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ComputopCreditCardPayment">
        <property name="payId" type="string" />
        <property name="merchantId" type="string" />
        <property name="transId" type="string" />
        <property name="amount" type="int" />
        <property name="currency" type="string" />
        <property name="mac" type="string" />
        <property name="urlSuccess" type="string" />
        <property name="urlFailure" type="string" />
        <property name="response" type="string" />
        <property name="urlNotify" type="string" />
        <property name="userData" type="string" />
        <property name="capture" type="string" />
        <property name="reqId" type="string" />
        <property name="plain" type="string" />
        <property name="custom" type="string" />
        <property name="expirationTime" type="string" />
        <property name="orderDesc" type="string" />
        <property name="customField" type="array" />
        <property name="paymentMethod" type="string" />
        <property name="items" type="Item[]" singular="item" />

        <property name="data" type="string" />
        <property name="len" type="int" />
        <property name="response" type="string" />
        <property name="txType" type="string" />
        <property name="url" type="string" />
        <property name="clientIp" type="string" />

        <property name="creditCardOrderResponse" type="ComputopCreditCardOrderResponse"/>

    </transfer>

    <transfer name="ComputopResponseHeader">
        <property name="status" type="string" />
        <property name="code" type="string" />
        <property name="isSuccess" type="bool" />
        <property name="description" type="string" />
        <property name="transId" type="string" />
        <property name="payId" type="string" />
        <property name="xid" type="string" />
        <property name="mid" type="string" />
        <property name="mac" type="string" />
    </transfer>

    <transfer name="ComputopCreditCardAuthorizeResponse">
        <property name="header" type="ComputopResponseHeader"/>
        <property name="len" type="string" />
        <property name="data" type="string" />
        <property name="mid" type="string" />
        <property name="payId" type="string" />
        <property name="mac" type="string" />
        <property name="xid" type="string" />
        <property name="transId" type="string" />
        <property name="status" type="string" />
        <property name="description" type="string" />
        <property name="code" type="string" />
        <property name="refNr" type="string" />
    </transfer>

    <transfer name="ComputopCreditCardOrderResponse">
        <property name="header" type="ComputopResponseHeader"/>
        <property name="len" type="string" />
        <property name="data" type="string" />
        <property name="mid" type="string" />
        <property name="payId" type="string" />
        <property name="xid" type="string" />
        <property name="transId" type="string" />
        <property name="status" type="string" />
        <property name="description" type="string" />
        <property name="code" type="string" />
        <property name="mac" type="string" />
        <property name="userData" type="string" />
        <property name="pcnr" type="string" />
        <property name="cCExpiry" type="string" />
        <property name="cCBrand" type="string" />
        <property name="maskedPan" type="string" />
        <property name="cavv" type="string" />
        <property name="eci" type="string" />
        <property name="type" type="string" />
        <property name="plain" type="string" />
        <property name="custom" type="string" />
    </transfer>

    <transfer name="Payment">
        <property name="computopCreditCard" type="ComputopCreditCardPayment" />
    </transfer>

    <transfer name="Order">
        <property name="computopCreditCard" type="ComputopCreditCardPayment" />
    </transfer>

</transfers>